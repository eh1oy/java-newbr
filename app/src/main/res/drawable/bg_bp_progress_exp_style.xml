<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@id/background">
        <shape android:shape="rectangle">
            <solid android:color="#ff6d7999" />
            <corners android:topRightRadius="30.0dip" android:bottomRightRadius="30.0dip" />
        </shape>
    </item>
    <item android:id="@id/progress">
        <clip android:gravity="start" android:clipOrientation="horizontal">
            <shape android:shape="rectangle">
                <solid android:color="#ffffba08" />
                <corners android:topRightRadius="30.0dip" android:bottomRightRadius="30.0dip" />
            </shape>
        </clip>
    </item>
</layer-list>
