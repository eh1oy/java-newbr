        <shape android:shape="rectangle">
            <solid android:color="#ff9e0202" />
            <corners android:radius="20.0dip" />
        </shape>
    </item>
    <item android:id="@android:id/background">
        <shape android:shape="rectangle">
            <gradient android:startColor="#ffff6317" android:endColor="#ffc50000" android:angle="0.0" android:type="linear" />
            <corners android:radius="20.0dip" />
        </shape>
    </item>
</ripple>
