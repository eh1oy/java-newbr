<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@id/background">
        <shape android:shape="rectangle">
            <solid android:color="#ff9da7c1" />
            <corners android:radius="@dimen/_41wdp" />
        </shape>
    </item>
    <item android:id="@id/progress">
        <clip android:gravity="start" android:clipOrientation="horizontal">
            <shape android:shape="rectangle">
                <gradient android:startColor="#ff7eff47" android:endColor="#ff40b110" android:angle="270.0" />
                <corners android:radius="@dimen/_41wdp" />
            </shape>
        </clip>
    </item>
</layer-list>
